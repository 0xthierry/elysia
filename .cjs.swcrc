{
	"module": {
		"type": "commonjs"
	},
	"exclude": ["(.*).(spec|test).(j|t)s$"],
	"jsc": {
		"target": "es2020",
		"parser": {
			"syntax": "typescript"
		},
		"minify": {
			"mangle": false,
			"compress": false
		}
	},
	"minify": false,
	"sourceMaps": true,
	"inlineSourcesContent": true,
	"env": {
		"targets": {
			"node": 16
		}
	}
}
